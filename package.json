{
  "name": "cabin.city",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npx prisma generate",
    "dev": "next dev",
    "build": "next build",
    "analyze": "NODE_ENV=development ANALYZE=true next build",
    "start": "next start",
    "prisma:dev": "CHECKPOINT_DISABLE=1 npx prisma db push && npx prisma generate",
    "format": "prettier --write .",
    "lint": "npm run lint:types && npm run lint:eslint && npm run lint:next",
    "lint:eslint": "eslint . --ext=.tsx,.ts",
    "lint:next": "next lint",
    "lint:types": "tsc --noEmit",
    "lint:contracts": "forge fmt --check",
    "lint:contracts:fix": "forge fmt",
    "codegen": "graphql-codegen",
    "contract:gen": "npx typechain --target ethers-v6 --out-dir ./generated/ethers **/abi/*.json; npx wagmi generate",
    "generate-routes": "npx tsx scripts/generate-routes.ts"
  },
