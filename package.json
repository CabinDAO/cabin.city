    "@unlock-protocol/paywall": "^0.6.5",
    "@vercel/analytics": "^1.0.2",
    "@wagmi/chains": "^1.8.0",
    "@wagmi/core": "^2.6.5",
    "alchemy-sdk": "^3.2.0",
    "axios": "^1.4.0",
    "crypto": "^1.0.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.3.1",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "ethers": "^6.11.1",
    "framer-motion": "^9.0.7",
    "iron-session": "^6.3.1",
    "jose": "^4.14.4",
    "multiparty": "^4.2.3",
    "next": "^13.5.6",
    "next-connect": "^0.13.0",
    "next-seo": "^6.0.0",
    "prop-types": "^15.8.1",
